@font-face
  font-family: Marcellus SC
  src: url('MarcellusSC-Regular.ttf')

body
 font-family: 'Marcellus SC', serif
 color: #584941
 text-shadow: 0 1px 1px rgba(255, 255, 255, .5)
 background-color: rgb(231, 231, 231)

#sic_from
  background-color #f5f5f5
  margin-bottom 12px
  padding 0 1em 1em 0

#sic_from h3
  display: inline-block
#sic_form_details
  background-color white
  margin-bottom 1em
  padding-bottom 1em
  border-bottom-left-radius 1em
  border-bottom-right-radius 1em

#sicfrom-title 
  .row
    padding-left 2em
    padding-bottom .5em
    span.add-on
      width 62px  

#ubicacionform
  float left
  padding-left 2em
  

#ms-custom-form-header
  padding: 0px 20px 0
  margin-bottom: 20px
  background-color: #f5f5f5
  border-top: 1px solid #e5e5e5
  *zoom: 1
  padding-bottom: 2.5em

p
  font-family: 'Arial', serif

.money-text
  color: #ffa500

.navbar-inner
  border-radius: 0px

.navbar-inner_min
  min-height: 25px
  font-size: 13px

.inline_for div
  display: inline

.navbar-inner_min > .navbar .nav > li > a
  padding: 2px 10px 0px 10px

#breadcrumb
  font-size: 12px

footer
  text-align: center

.imagecenter
  display: block
  margin-left: auto
  margin-right: auto

.ms-form-right-detail
  padding-bottom: 6px
  background-color: rgb(255, 255, 255)
  padding-left: 12px
  margin-bottom: 5px

[class^="msicon-"],[class*="msicon-"]
  display: inline-block
  width: 30px
  height: 30px
  margin-top: 0px
  *margin-right: 0em
  line-height: 5px
  vertical-align: text-top
  background-image: url("../img/iconos-microsip.png")
  background-position: 30px 30px
  background-repeat: no-repeat

.text-center
  text-align: center

.msicon-white,
.nav-pills > .active > a > [class^="msicon-"],
.nav-pills > .active > a > [class*=" msicon-"],
.nav-list > .active > a > [class^="msicon-"],
.nav-list > .active > a > [class*=" msicon-"],
.navbar-inverse .nav > .active > a > [class^="msicon-"],
.navbar-inverse .nav > .active > a > [class*=" msicon-"],
.dropdown-menu > li > a:hover > [class^="msicon-"],
.dropdown-menu > li > a:focus > [class^="msicon-"],
.dropdown-menu > li > a:hover > [class*=" msicon-"],
.dropdown-menu > li > a:focus > [class*=" msicon-"],
.dropdown-menu > .active > a > [class^="msicon-"],
.dropdown-menu > .active > a > [class*=" msicon-"],
.dropdown-submenu:hover > a > [class^="msicon-"],
.dropdown-submenu:focus > a > [class^="msicon-"],
.dropdown-submenu:hover > a > [class*=" msicon-"],
.dropdown-submenu:focus > a > [class*=" msicon-"]
  background-image: url("../img/iconos-microsip.png")

.msicon-admin
  background-position: -3px -367px

.msicon-logout
  background-position: -4px -334px

.msicon-inventarios
  background-position: -4px -38px

.msicon-ventas
  background-position: -5px -3px

.msicon-cuentas_por_pagar
  background-position: -4px -101px

.msicon-cuentas_por_cobrar
  background-position: -4px -68px

.msicon-compras
  background-position: -4px -133px

.msicon-punto_de_venta
  background-position: -3px -263px

.msicon-contabilidad
  background-position: -4px -198px

.msicon-reportes
  background-position: -4px -164px

.msicon-home
  background-position: -4px -230px

.pagecontent
  background-color: rgba(255, 255, 255, 1)
  text-align: left
  padding: 1em
  padding-top: 0em
  padding-bottom: 2em
  margin-left: 1em
  margin-right: 1em

.table th, .table td
  padding: 0px

.mainmenu
  text-align: center

#IrArriba
  position: fixed
  bottom: 30px
  right: 30px

.navbar
  margin-bottom: -1px

.autocomplete-light-widget
  font-size: 14px

.text-error
  font-size: 12px

#IrArriba span
  width: 60px
  height: 60px
  display: block
  background: url(../img/flecha-arriba.png) no-repeat center center

.label_min
  display: none

@media only screen and (max-width : 980px)
  .label_min
    display: block
    padding-top: .4em

.small_screen
  display: none

@media only screen and (max-width : 800px)
  .small_screen
    display: inline-block

  td
    line-height: 3em

